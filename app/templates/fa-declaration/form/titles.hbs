{{#nop-page title='Titles'
            class='titles-page'
            hint='These are the titles pertaining to this declaration. This declaration remains valid for the titles(s) listed in the declaration unless replaced by a new declaration for the title(s).'}}
  {{nop-titles titles=model.titles}}

  {{#nop-actions}}
    {{#link-to back}}
      <button>Back</button>
    {{/link-to}}
    {{#link-to next}}
      <button class='primary'>Next</button>
    {{/link-to}}
  {{/nop-actions}}
{{/nop-page}}
